new pHairSlot[MAX_PLAYERS] = { -1, -1, ...};

hook OnPlayerConnect(playerid)
{
	pHairSlot[playerid] = -1;
	return 1;
}	

CMD:hair(playerid, params[])
{
    new 
        skin = GetPlayerSkin(playerid),
        hair;

    if (playerData[playerid][pHair] == 0)
        return SendClientMessage(playerid, COLOR_GRAD1, "คุณต้องได้รับสิทธ์ในการเป็นลูกค้าของร้านตัดผม Barber (สิทธ์หาจากกล่อง Christmas V.1)");

	if (sscanf(params, "d", hair))
	{
		SendClientMessage(playerid, COLOR_GRAD1, "การใช้: /hair [เลือกทรงผมที่ต้องการ]");
		SendClientMessage(playerid, COLOR_YELLOW, "|1 ผมสีดำ (Hair3) |2 ผมสีดำ (Hair2) |3 ลบทรงผมออก (Deleted)");
		return 1;
	}

	switch (hair)
	{
		case 1: 
		{
            SetPlayerAttachedObject(playerid, GetSlot(playerid), 19077, 2, SkinOffSet[skin][0], SkinOffSet[skin][1], SkinOffSet[skin][2], SkinOffSet[skin][3], SkinOffSet[skin][4], SkinOffSet[skin][5], SkinOffSet[skin][8], SkinOffSet[skin][8], SkinOffSet[skin][8]);
            SendClientMessage(playerid, COLOR_GRAD1, "  ช่างตัดผมได้ทำการตัดแต่งทรงผมให้กับคุณตามที่คุณต้องการแล้ว");
        }
        case 2:
        {
            SetPlayerAttachedObject(playerid, GetSlot(playerid), 18975, 2, SkinOffSet[skin][0], SkinOffSet[skin][1], SkinOffSet[skin][2], SkinOffSet[skin][3], SkinOffSet[skin][4], SkinOffSet[skin][5], SkinOffSet[skin][8], SkinOffSet[skin][8], SkinOffSet[skin][8]);
            SendClientMessage(playerid, COLOR_GRAD1, "  ช่างตัดผมได้ทำการตัดแต่งทรงผมให้กับคุณตามที่คุณต้องการแล้ว");
        }
        case 3:
        {
            RemovePlayerAttachedObject(playerid, pHairSlot[playerid]);
            pHairSlot[playerid] = -1;
            SendClientMessage(playerid, COLOR_GRAD1, "  ช่างตัดผมได้ทำการจัดแต่งทรงผมให้กับคุณใหม่อีกครั้ง, กลับไปเป็นแบบ Classic");
        }
    }

	return 1;
}

GetSlot(playerid)
{	
	for(new i = 0; i < MAX_PLAYER_ATTACHED_OBJECTS; ++i)
	{
		if(!IsPlayerAttachedObjectSlotUsed(playerid, i)) 
		{
			if(pHairSlot[playerid] == -1)
			{
				pHairSlot[playerid] = i;
				return pHairSlot[playerid];
			}	
			return pHairSlot[playerid];
		}	
	}
	return -1;
}	



stock strtok(const string[], &index)
{
	new length = strlen(string);
	while ((index < length) && (string[index] <= ' '))
	{
		index++;
	}

	new offset = index;
	new result[20];
	while ((index < length) && (string[index] > ' ') && ((index - offset) < (sizeof(result) - 1)))
	{
		result[index - offset] = string[index];
		index++;
	}
	result[index - offset] = EOS;
	return result;
}
